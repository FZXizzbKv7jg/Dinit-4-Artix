type            = process
command         = /usr/bin/gitea web -w /var/lib/gitea --config /etc/gitea/app.ini
smooth-recovery = true
logfile         = /var/log/dinit/gitea.log
run-as          = gitea
load-options    = export-passwd-vars
working-dir	= /var/lib/gitea
working-dir	= /path/to/gitrepository
depends-on	= network
after		= mysqld
after		= redis
